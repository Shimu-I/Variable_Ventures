#include <iostream>
using namespace std;



int binarySearch(int array[], int n, int key){
    int start = 0;
    int end = n-1;
    int mid = start + ((end - start)/2);

    while( start <= end ){
        if( array[mid] == key ){
            return mid;
        }
        else if( array[mid] < key){
            start = mid + 1;
        }
        else if( array[mid] > key){
            end = mid - 1;
        }
        mid = start - ((end - start)/2);
    }
    return -1;
}

int main(){
    int array[6] = {3, 6, 11, 45, 76, 89};

    int key;
    key = binarySearch(array, 6, 11);

    if( key != -1){
        cout << " Found the key at index : " << key << endl;
    }
    else{
        cout << "Not found" << endl;
    }
    
}